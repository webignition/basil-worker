{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2.5",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-pdo_pgsql": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "guzzlehttp/guzzle": "^7.1",
        "psr/http-client": "^1.0",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/messenger": "5.1.*",
        "symfony/yaml": "5.1.*",
        "webignition/basil-compiler-models": "^0.14.0",
        "webignition/tcp-cli-proxy-client": "^0.11.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "mockery/mockery": "^1.4",
        "php-mock/php-mock": "^2.2",
        "php-mock/php-mock-mockery": "^1.3",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.43",
        "phpstan/phpstan-doctrine": "^0.12.19",
        "phpstan/phpstan-mockery": "^0.12.7",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpstan/phpstan-symfony": "^0.12.7",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "5.1.*",
        "symfony/maker-bundle": "^1.21",
        "symplify/easy-coding-standard": "^8.3",
        "webignition/object-reflector": "^0.2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs": [
            "./vendor/bin/ecs check src tests --ansi --config ecs.php",
            "./vendor/bin/phpcs"
        ],
        "cs-fix": [
            "./vendor/bin/ecs check src tests --ansi --config ecs.php --fix"
        ],
        "static-analysis": "./vendor/bin/phpstan analyse --ansi",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@cs",
            "@static-analysis",
            "@test"
        ],
        "integration-setup": "docker-compose --env-file tests/.env -f tests/docker-compose.yml up -d",
        "integration-status": "docker-compose --env-file tests/.env -f tests/docker-compose.yml ps",
        "integration-teardown": "docker-compose --env-file tests/.env -f tests/docker-compose.yml down"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.1.*"
        }
    }
}
